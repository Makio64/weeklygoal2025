{
	"name": "makio-starter",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"target": "esnext",
	"scripts": {
		"dev": "vite --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"lint": "eslint src --fix",
		"optimizeImg": "node nodejs-tools/optimizeImg.js",
		"optimizeKtx": "node nodejs-tools/optimizeKtx.js",
		"optimizeVideo": "node nodejs-tools/optimizeVideo.js",
		"optimizeAudio": "node nodejs-tools/optimizeAudio.js",
		"copyDropbox": "node nodejs-tools/copyDropbox.js",
		"updateTrad": "node nodejs-tools/updateTrad.mjs",
		"prepare": "husky",
		"cap:sync": "pnpm build && npx cap sync && node nodejs-tools/incrementBuild.js",
		"cap:open:ios": "npx cap open ios",
		"cap:run:ios": "pnpm cap:sync && npx cap run ios --target=480F5924-C1A0-4C18-8508-204E17B48404",
		"ios": "pnpm cap:run:ios"
	},
	"dependencies": {
		"@capacitor/cli": "^7.4.3",
		"@capacitor/core": "^7.4.3",
		"@capacitor/ios": "^7.4.3",
		"@capacitor/local-notifications": "^7.0.3",
		"@capacitor/preferences": "^7.0.2",
		"animejs": "^4.2.2",
		"stats-gl": "^3.6.0",
		"three": "^0.180.0",
		"vue": "^3.5.22",
		"vue-tiny-router": "^1.7.0",
		"vue-tiny-translation": "^1.0.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@stylistic/eslint-plugin": "^5.5.0",
		"@vitejs/plugin-vue": "^6.0.1",
		"airtable": "^0.12.2",
		"copy-and-watch": "^0.1.8",
		"cssnano": "^7.1.1",
		"dotenv": "^17.2.3",
		"eslint": "^9.38.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unused-imports": "^4.2.0",
		"eslint-plugin-vue": "^10.5.1",
		"fast-glob": "^3.3.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.4",
		"postcss": "^8.5.6",
		"rollup-plugin-visualizer": "^6.0.5",
		"sharp": "^0.34.4",
		"stylus": "^0.64.0",
		"terser": "^5.44.0",
		"unplugin-auto-import": "^20.2.0",
		"unplugin-vue-components": "^29.1.0",
		"vite": "^7.1.10",
		"vite-plugin-brotli-size": "^1.1.0",
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vite-plugin-mkcert": "^1.17.9",
		"vite-plugin-qrcode": "^0.3.0",
		"vite-plugin-tsl-operator": "^1.4.3",
		"vue-eslint-parser": "^10.2.0"
	},
	"pnpm": {
		"overrides": {
			"braces@<3.0.3": ">=3.0.3",
			"micromatch@<4.0.8": ">=4.0.8",
			"rollup@>=4.0.0 <4.22.4": ">=4.22.4",
			"cross-spawn@<7.0.5": ">=7.0.5",
			"nanoid@<3.3.8": ">=3.3.8",
			"brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
			"brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
			"form-data@>=4.0.0 <4.0.4": ">=4.0.4",
			"axios@<1.12.0": ">=1.12.0"
		},
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp"
		]
	},
	"lint-staged": {
		"*.{js,jsx,vue}": [
			"eslint --fix"
		]
	},
	"packageManager": "pnpm@10.18.3"
}
